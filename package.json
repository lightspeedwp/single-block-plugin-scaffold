{
	"name": "{{slug}}",
	"version": "{{version}}",
	"description": "{{description}}",
	"author": "{{author}}",
	"license": "{{license}}",
	"main": "build/index.js",
	"keywords": [
		"wordpress",
		"gutenberg",
		"block",
		"{{slug}}",
		"{{namespace}}"
	],
	"homepage": "https://github.com/{{author}}/{{slug}}",
	"repository": {
		"type": "git",
		"url": "https://github.com/{{author}}/{{slug}}.git"
	},
	"bugs": {
		"url": "https://github.com/{{author}}/{{slug}}/issues"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	},
	"scripts": {
		"build": "wp-scripts build",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint": "npm run lint:js && npm run lint:css && npm run lint:pkg-json",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js",
		"test": "npm run test:unit && npm run test:e2e",
		"env": "wp-env",
		"env:start": "wp-env start",
		"env:stop": "wp-env stop",
		"env:restart": "wp-env start --update",
		"env:destroy": "wp-env destroy",
		"env:logs": "wp-env logs",
		"env:cli": "wp-env run cli wp",
		"update-version": "node bin/update-version.js"
	},
	"devDependencies": {
		"@wordpress/scripts": "^27.9.0",
		"@wordpress/env": "^9.0.0",
		"@wordpress/jest-preset-default": "^12.0.0",
		"@wordpress/e2e-test-utils-playwright": "^0.25.0",
		"@wordpress/stylelint-config": "^22.0.0",
		"@wordpress/prettier-config": "^4.0.0",
		"playwright": "^1.45.0"
	},
	"dependencies": {
		"@wordpress/block-editor": "^12.0.0",
		"@wordpress/blocks": "^12.0.0",
		"@wordpress/components": "^28.0.0",
		"@wordpress/i18n": "^4.0.0",
		"@wordpress/icons": "^9.0.0",
		"@wordpress/postcss-plugins-preset": "^4.0.0"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"prettier": "@wordpress/prettier-config",
	"wp-env": {
		"plugins": [
			"."
		],
		"themes": [
			"https://wordpress.org/latest.zip"
		]
	}
}